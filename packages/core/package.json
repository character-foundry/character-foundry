{
  "name": "@character-foundry/core",
  "version": "0.1.2",
  "description": "Core utilities for character card handling",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./zip": {
      "import": {
        "types": "./dist/zip.d.ts",
        "default": "./dist/zip.js"
      },
      "require": {
        "types": "./dist/zip.d.cts",
        "default": "./dist/zip.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "fflate": "^0.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
