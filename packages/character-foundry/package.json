{
  "name": "@character-foundry/character-foundry",
  "version": "0.1.3",
  "description": "Universal TypeScript library for AI character card formats - meta package",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.js"
    },
    "./schemas": {
      "types": "./dist/schemas.d.ts",
      "import": "./dist/schemas.js"
    },
    "./png": {
      "types": "./dist/png.d.ts",
      "import": "./dist/png.js"
    },
    "./charx": {
      "types": "./dist/charx.d.ts",
      "import": "./dist/charx.js"
    },
    "./voxta": {
      "types": "./dist/voxta.d.ts",
      "import": "./dist/voxta.js"
    },
    "./lorebook": {
      "types": "./dist/lorebook.d.ts",
      "import": "./dist/lorebook.js"
    },
    "./loader": {
      "types": "./dist/loader.d.ts",
      "import": "./dist/loader.js"
    },
    "./exporter": {
      "types": "./dist/exporter.d.ts",
      "import": "./dist/exporter.js"
    },
    "./normalizer": {
      "types": "./dist/normalizer.d.ts",
      "import": "./dist/normalizer.js"
    },
    "./tokenizers": {
      "types": "./dist/tokenizers.d.ts",
      "import": "./dist/tokenizers.js"
    },
    "./media": {
      "types": "./dist/media.d.ts",
      "import": "./dist/media.js"
    },
    "./federation": {
      "types": "./dist/federation.d.ts",
      "import": "./dist/federation.js"
    },
    "./app-framework": {
      "types": "./dist/app-framework.d.ts",
      "import": "./dist/app-framework.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@character-foundry/core": "workspace:^",
    "@character-foundry/schemas": "workspace:^",
    "@character-foundry/png": "workspace:^",
    "@character-foundry/charx": "workspace:^",
    "@character-foundry/voxta": "workspace:^",
    "@character-foundry/lorebook": "workspace:^",
    "@character-foundry/loader": "workspace:^",
    "@character-foundry/exporter": "workspace:^",
    "@character-foundry/normalizer": "workspace:^",
    "@character-foundry/tokenizers": "workspace:^",
    "@character-foundry/media": "workspace:^",
    "@character-foundry/federation": "workspace:^",
    "@character-foundry/app-framework": "workspace:^"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
